@article{Wait1998,
abstract = {Radio-wave transmission over the surface of the Earth is a subject of enquiry going back to the beginning of the century. In this review, an attempt is made to describe the ground-wave mechanism that is omni-present. We first call attention to the early analytical contributions of Zenneck and Sommerfeld, based on a flat-Earth model. The subsequent controversies, particularly with regard to the role of the Zenneck surface wave, are outlined. Further developments by other pioneers, such as van der Pol, Fock, Bremmer, Norton, and Millington, are reviewed, and an attempt is made to put these in a modern context. We also show that the trapped surface wave can be a significant contribution to the total ground-wave field, when the Earth boundary is sufficiently inductive. Mixed-path theory and confirming model tests by Ray King are described briefly, along with calculated propagation curves for twoand three-section paths. The bibliography includes references to related topics, such as tropospheric refraction and topographic influences View full abstract},
author = {Wait, James R.},
doi = {10.1109/74.735961},
file = {:E$\backslash$:/Users/Amauri/Documents/UnB/Refer{\^{e}}ncias/wait1998.pdf:pdf},
