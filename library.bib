title = {{Estudo da Influ{\^{e}}ncia da Corrente Alternada na Corros{\~{a}}o em Dutos Met{\'{a}}licos Enterrados}},
type = {Disserta{\c{c}}{\~{a}}o (Mestrado)},
year = {2008}
}
